{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "functions": {
        "app/**/*.py": {
            "excludeFiles": "{.venv,__pycache__,*.pyc,tests,*.log,uploads}/**"
        }
    }
}